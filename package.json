  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayajs/cli.git"
  },
  "keywords": [
    "cli",
    "mayajs"
  ],
  "author": "Mark Anthony C. Ignacio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mayajs/cli/issues"
  },
  "homepage": "https://github.com/mayajs/cli#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^3.0.0",
    "figlet": "^1.2.4",
    "listr": "^0.14.3",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/figlet": "^1.2.0",
    "@types/listr": "^0.14.2",
    "@types/node": "^12.7.2",
    "@types/shelljs": "^0.8.5",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.5.1",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0"
  }
