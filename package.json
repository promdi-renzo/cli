{
  "name": "@mayajs/cli",
  "version": "0.1.0",
  "description": "MayaJS Command Line Interface",
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "files"
  ],
  "bin": {
    "maya": "./index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
